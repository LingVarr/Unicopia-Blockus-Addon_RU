{
  "schemaVersion": 1,
  "id": "unicopia-blockus",
  "version": "${version}",
  "name": "Unicopia",
  "description": "Blockus Blocks for Unicopia!",
  "authors": [ "Sollace" ],
  "contributors": [ ],
  "contact": {
    "sources": "https://github.com/Sollace/Unicopia-Blockus-Addon",
